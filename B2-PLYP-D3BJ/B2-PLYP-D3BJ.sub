cd /home/zw18965/work/HFREQ2014/B2-PLYP-D3BJ


source $HOME/.bashrc

#Setup scratch directory
tmp=${TMPDIR}
cp * ${tmp}

# Run entos
cd ${tmp}
export MKL_THREADING_LAYER=""
conda activate p4env

for file in *.in; do
  filename=${file%.in}
  if [[ ! -f ${filename}.out ]]; then
    psi4 -n 24 -i "${filename}.in" -o  "${filename}.out"
    cp ${filename}.* /home/zw18965/work/HFREQ2014/B2-PLYP-D3BJ
  fi
done

cd /home/zw18965/work/HFREQ2014/B2-PLYP-D3BJ
rm -Rf 
